<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_delta"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="5dp"
    android:paddingLeft="4dp"
    android:paddingRight="4dp"
    android:paddingTop="2dp"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.feriteja.projectpasmartgarbage.DeltaActivity"
    tools:showIn="@layout/app_bar_delta"
    android:background="#eff6f7">


    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/testsa" />

    <ExpandableListView
        android:layout_width="match_parent"
        android:groupIndicator="@null"
        android:layout_height="300dp"
        android:id="@+id/expandableListView"
        android:layout_below="@+id/searchView1"
        android:layout_marginRight="3dp"
        android:layout_marginLeft="3dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:dividerHeight="6dp"
        android:childDivider="#e3e8e8"
        android:divider="#eff6f7"/>

    <SearchView
        android:layout_width="240dp"
        android:id="@+id/searchView1"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Spinner
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:id="@+id/spinner2"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
